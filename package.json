{
  "scripts": {
    "setup-project": "yarn && cd frontend && yarn && cd .. && altv-pkg release",
    "build:server": "node build/server",
    "build:client": "node build/client",
    "build:frontend": "cd frontend && yarn build",
    "dev:server": "node build/server -dev",
    "dev:client": "node build/client -dev",
    "before-any": "yarn && node build/before-copy",
    "after-build-copy": "node build/after-copy",
    "dev": "yarn before-any && concurrently \"yarn:dev:*\" \"yarn:tsc\" \"cd frontend && yarn dev\" -c \"magenta.bold,cyan.bold\"",
    "build": "yarn before-any && concurrently \"yarn:build:*\" && yarn after-build-copy",
    "upd-altv-types": "yarn add @altv/types-shared@latest @altv/types-client@latest @altv/types-server@latest @altv/types-natives@latest",
    "tsc:server": "cd src/server && npx tsc -w",
    "tsc:client": "cd src/client && npx tsc -w",
    "tsc": "concurrently \"yarn:tsc:*\" -c \"magenta.bold,cyan.bold\""
  },
  "type": "module",
  "dependencies": {
    "@altv/types-client": "^2.8.8",
    "@altv/types-natives": "^1.5.4",
    "@altv/types-server": "^2.9.2",
    "@altv/types-shared": "^1.5.9",
    "@altv/types-webview": "^1.0.6",
    "altv-esbuild": "^0.4.12",
    "altv-pkg": "^2.3.1",
    "altv-xrpc-client": "^0.3.2",
    "altv-xrpc-server": "^0.3.3",
    "altv-xrpc-shared": "^0.3.2",
    "altv-xrpc-shared-types": "^0.3.2",
    "concurrently": "^7.3.0",
    "esbuild": "^0.17.10",
    "typescript": "^4.9.5"
  }
}
